{
  "name": "seraphin-website-factory",
  "version": "1.1.1",
  "private": true,
  "description": "A static blog based on Contentful",
  "author": "Robin Dierckx",
  "scripts": {
    "production-deprecated": "aws s3 cp dist/ s3://staging.auto-assurance.be --recursive --profile seraphin",
    "postproduction-deprecated": "aws cloudfront create-invalidation --distribution-id EFDPO45IGWHOD --paths '/*' --profile seraphin",
    "import-data-deprecated": "node ./bin/download-content-model.js && contentful-import --content-file ./data/blog/contentful-export.json",
    "backup": "bash scripts/contentful-backup.sh",
    "build": "nuxt build --spa",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "deploy": "bash scripts/deploy.sh",
    "dev": "bash scripts/dev.sh",
    "generate": "nuxt generate",
    "precommit": "npm run lint",
    "start": "bash scripts/start.sh",
    "checkout": "bash scripts/checkout.sh",
    "use": "bash scripts/use.sh",
    "launch": "bash scripts/launch.sh",
    "warmup": "bash scripts/warmup.sh",
    "everything": "bash scripts/everything.sh",
    "import-content-types": "bash ./scripts/content-type-migration-script.sh"
  },
  "dependencies": {
    "@nuxtjs/component-cache": "^1.1.5",
    "@nuxtjs/gtm": "^2.2.3",
    "@nuxtjs/robots": "^2.4.2",
    "@nuxtjs/sentry": "^3.3.1",
    "@nuxtjs/sitemap": "^2.0.1",
    "@nuxtjs/style-resources": "^1.0.0",
    "@vue/cli-service": "^4.1.2",
    "axios": "^0.19.1",
    "babel-runtime": "^6.26.0",
    "contentful": "^4.3.0",
    "contentful-management": "^3.4.0",
    "dotenv": "^8.2.0",
    "js-md5": "^0.7.3",
    "lazysizes": "^5.2.2",
    "nuxt": "2.12.2",
    "nuxt-compress": "^2.0.0",
    "nuxt-jsonld": "^1.4.10",
    "nuxt-trailingslash-module": "^1.1.0",
    "nuxt-webfontloader": "^1.1.0",
    "vue-carousel": "0.18.0",
    "vue-cli-plugin-s3-deploy": "3.0.0",
    "vue-client-only": "^2.0.0",
    "vue-cookies": "^1.7.0",
    "vue-lazy-hydration": "^1.0.0-beta.12",
    "vue-lazyload": "^1.3.3",
    "vue-markdown": "^2.1.3",
    "vue-star-rating": "^1.6.1",
    "vue-toc": "^0.0.1"
  },
  "devDependencies": {
    "babel-eslint": "10.0.3",
    "contentful-import": "^4.5.4",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "6.8.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "3.0.3",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "gh-pages": "^1.0.0",
    "listr": "^0.12.0",
    "node-fetch": "2.6.0",
    "node-sass": "^4.13.1",
    "now": "^5.3.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^8.0.2",
    "tar": "^4.0.1"
  },
  "config": {
    "nuxt": {
      "host": "0.0.0.0"
    }
  }
}
